{
  "name": "es6-webpack2-boilerplate",
  "version": "1.0.0",
  "description": "a boilerpalte for web devolopment with es6 and webpack2",
  "main": "index.js",
  "scripts": {
    "start": "webpack-dashboard -- webpack-dev-server --config config/config.dev.js",
    "eslint": "eslint ./config ./src",
    "eslint:watch": "esw -w ./config ./src",
    "sasslint": "sass-lint -v -q",
    "clean": "rm -rf dist",
    "build": "npm run clean && npm run eslint && npm run sasslint && webpack --progress --color --config config/config.prod.js",
    "postpublish": "git push --all && git push origin --tags"
  },
  "keywords": [
    "es6",
    "webpack2",
    "hrm"
  ],
  "author": "mhaw",
  "license": "ISC",
  "devDependencies": {
    "autoprefixer": "^6.4.1",
    "babel-core": "^6.14.0",
    "babel-eslint": "^6.1.2",
    "babel-loader": "^6.2.5",
    "babel-preset-es2015": "^6.14.0",
    "babel-preset-react": "^6.11.1",
    "babel-preset-stage-2": "^6.13.0",
    "css-loader": "^0.25.0",
    "eslint": "^3.5.0",
    "eslint-config-standard": "^6.0.0",
    "eslint-config-standard-react": "^4.0.1",
    "eslint-plugin-promise": "^2.0.1",
    "eslint-plugin-react": "^6.2.0",
    "eslint-plugin-standard": "^2.0.0",
    "eslint-watch": "^2.1.14",
    "extract-text-webpack-plugin": "^2.0.0-beta.4",
    "file-loader": "^0.9.0",
    "html-webpack-plugin": "^2.22.0",
    "json-loader": "^0.5.4",
    "node-sass": "^3.9.3",
    "postcss-loader": "^0.13.0",
    "sass-lint-rules": "^1.0.2",
    "sass-loader": "^4.0.2",
    "style-loader": "^0.13.1",
    "url-loader": "^0.5.7",
    "webpack": "^2.1.0-beta.22",
    "webpack-dashboard": "^0.1.8",
    "webpack-dev-server": "^2.1.0-beta.4",
    "webpack-merge": "^0.14.1"
  }
}
